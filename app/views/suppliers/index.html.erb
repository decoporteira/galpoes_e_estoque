<section id="suppliers">
  <h2>Fornecedores</h2>
  <%= link_to('Cadastrar Fornecedor', new_supplier_path)  %>
  <% @suppliers.each do |supplier| %>
    <div>
      <h3><%= link_to(supplier.corporate_name, supplier) %> </h3>
      <dl>
        <dt>Corporate Name:</dt>
        <dd><%= supplier.brand_name %></dd>
        <dt>Brand Name:</dt>
        <dd><%= supplier.corporate_name %></dd>
        <dt>Registration Number:</dt>
        <dd><%= supplier.registration_number %></dd>
        <dt>City:</dt>
        <dd><%= supplier.city %></dd>
      </dl>
    </div>
  <% end %>
  <% if @suppliers.empty? %>
    <p>NÃ£o existem fornecedores cadastrados.</p>
  <% end%>
</section>
